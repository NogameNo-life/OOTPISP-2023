<Window x:Class="lab3.Task3Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:lab3"
        mc:Ignorable="d"
        Title="Task3Window" SizeToContent="Height" Width="300">
    <StackPanel>
        <TextBox Name="InputTextBox" Margin="3"></TextBox>
        <Label Name="ResultLabel" Content="Input:" Margin="3"></Label>
        <Button Name="RunButton" Click="RunButton_OnClick" Content="Run" Margin="3"></Button>
    </StackPanel>
</Window>
